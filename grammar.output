Grammar

    0 $accept: programa $end

    1 programa: lista_definicoes

    2 lista_definicoes: definicao
    3                 | definicao lista_definicoes

    4 definicao: def_variavel
    5          | def_funcao

    6 def_variavel: ID ':' tipo ';'

    7 tipo: tipo_primitivo
    8     | tipo '[' ']'

    9 tipo_primitivo: TK_INT
   10               | TK_FLOAT
   11               | TK_CHAR
   12               | TK_VOID

   13 def_funcao: ID '(' parametros ')' ':' tipo bloco

   14 parametros: %empty
   15           | lista_params

   16 lista_params: parametro
   17             | parametro ',' lista_params

   18 parametro: ID ':' tipo

   19 bloco: '{' lista_def_var lista_comandos '}'

   20 lista_def_var: %empty
   21              | lista_def_var lista_def_var2

   22 lista_def_var2: def_variavel

   23 lista_comandos: %empty
   24               | comando lista_comandos

   25 comando: bloco
   26        | '@' expressao ';'
   27        | TK_RETURN expressao ';'
   28        | TK_RETURN ';'
   29        | exp_variavel '=' expressao ';'
   30        | TK_IF expressao bloco
   31        | TK_IF expressao bloco TK_ELSE bloco
   32        | TK_WHILE expressao bloco
   33        | chamada ';'

   34 exp_variavel: ID
   35             | expressao_base '[' expressao ']'

   36 chamada: ID '(' lista_exp ')'

   37 lista_exp: %empty
   38          | lista_exp2

   39 lista_exp2: expressao
   40           | expressao ',' lista_exp2

   41 expressao: exp_or

   42 exp_or: exp_and
   43       | exp_or TK_OR exp_and

   44 exp_and: exp_cmp
   45        | exp_and TK_AND exp_cmp

   46 exp_cmp: exp_add
   47        | exp_cmp TK_EQUAL exp_add
   48        | exp_cmp TK_NOTEQUAL exp_add
   49        | exp_cmp TK_LESSEQUAL exp_add
   50        | exp_cmp TK_GREATEREQUAL exp_add
   51        | exp_cmp '<' exp_add
   52        | exp_cmp '>' exp_add

   53 exp_add: exp_mult
   54        | exp_add '+' exp_mult
   55        | exp_add '-' exp_mult

   56 exp_mult: exp_unaria
   57         | exp_mult '*' exp_unaria
   58         | exp_mult '/' exp_unaria

   59 exp_unaria: exp_as
   60           | '-' exp_unaria
   61           | '!' exp_unaria

   62 exp_as: expressao_base
   63       | expressao_base TK_AS tipo

   64 expressao_base: constante
   65               | exp_variavel
   66               | '(' expressao ')'
   67               | chamada
   68               | TK_NEW tipo '[' expressao ']'

   69 constante: TK_DEC
   70          | TK_REAL
   71          | TK_STRING

   72 ID: TK_ID


