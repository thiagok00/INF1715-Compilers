Grammar

    0 $accept: programa $end

    1 programa: lista_definicoes

    2 lista_definicoes: definicao
    3                 | definicao lista_definicoes

    4 definicao: def_variavel
    5          | def_funcao

    6 def_variavel: ID ':' tipo ';'

    7 tipo: tipo_primitivo
    8     | tipo '[' ']'

    9 tipo_primitivo: TK_INT
   10               | TK_FLOAT
   11               | TK_CHAR
   12               | TK_VOID

   13 def_funcao: ID '(' parametros ')' ':' tipo bloco

   14 parametros: %empty
   15           | lista_params

   16 lista_params: parametro
   17             | parametro ',' lista_params

   18 parametro: ID ':' tipo

   19 bloco: '{' lista_def_var lista_comandos '}'

   20 lista_def_var: %empty
   21              | lista_def_var def_variavel

   22 lista_comandos: %empty
   23               | comando lista_comandos

   24 comando: bloco
   25        | '@' expressao ';'
   26        | TK_RETURN expressao ';'
   27        | TK_RETURN ';'
   28        | variavel '=' expressao ';'
   29        | TK_IF expressao bloco
   30        | TK_IF expressao bloco TK_ELSE bloco
   31        | TK_WHILE expressao bloco
   32        | chamada ';'

   33 variavel: ID
   34         | expressao_base '[' expressao ']'

   35 chamada: ID '(' lista_exp ')'

   36 lista_exp: %empty
   37          | lista_exp2

   38 lista_exp2: expressao
   39           | expressao ',' lista_exp2

   40 expressao: exp_or

   41 exp_or: exp_and
   42       | exp_or TK_OR exp_and

   43 exp_and: exp_cmp
   44        | exp_and TK_AND exp_cmp

   45 exp_cmp: exp_add
   46        | exp_cmp TK_EQUAL exp_add
   47        | exp_cmp TK_NOTEQUAL exp_add
   48        | exp_cmp TK_LESSEQUAL exp_add
   49        | exp_cmp TK_GREATEREQUAL exp_add
   50        | exp_cmp '<' exp_add
   51        | exp_cmp '>' exp_add

   52 exp_add: exp_mult
   53        | exp_add '+' exp_mult
   54        | exp_add '-' exp_mult

   55 exp_mult: exp_unaria
   56         | exp_mult '*' exp_unaria
   57         | exp_mult '/' exp_unaria

   58 exp_unaria: exp_as
   59           | '-' exp_unaria
   60           | '!' exp_unaria

   61 exp_as: expressao_base
   62       | expressao_base TK_AS tipo

   63 expressao_base: TK_DEC
   64               | TK_REAL
   65               | TK_STRING
   66               | variavel
   67               | '(' expressao ')'
   68               | chamada
   69               | TK_NEW tipo '[' expressao ']'

   70 ID: TK_ID


